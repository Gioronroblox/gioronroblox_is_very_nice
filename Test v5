    debounce = false
    idle = false
    walk = false
    walk1 = false
    

    for i = 1,5 do
    LeftShoulder.C0 = LeftShoulder.C0:lerp(CFrame.new(LeftShoulder.C0.p)*CFrame.Angles(math.rad(120.951), math.rad(-41.081), math.rad(49.504)),.5) --LeftUpperArm
