			end
		end)
        for i = 1,30 do
    LeftShoulder.C0 = LeftShoulder.C0:lerp(CFrame.new(LeftShoulder.C0.p)*CFrame.Angles(math.rad(30.023), math.rad(-3.61), math.rad(-39.305)),.3) --LeftUpperArm
