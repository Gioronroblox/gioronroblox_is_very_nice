Swordmesh = mesh("SpecialMesh",Sword,Enum.MeshType.FileMesh,"http://www.roblox.com/asset/?id=0",Vector3.new(0, 0, 0),Vector3.new(1, 1, 1))
Swordmesh.TextureId = "http://www.roblox.com/asset/?id=0"
