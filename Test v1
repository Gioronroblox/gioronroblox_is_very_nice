for _, BasePart in next, Character:GetChildren() do
    if BasePart:IsA("BasePart") then
        for _, Name in pairs(BaseParts) do
            if BasePart.Name == Name then
                local Transparency = BasePart.Transparency
                
                BasePart.LocalTransparencyModifier = Transparency
                
                BasePart:GetPropertyChangedSignal("LocalTransparencyModifier"):Connect(function()
                    BasePart.LocalTransparencyModifier = Transparency
                end)
            end
        end
    end
end






