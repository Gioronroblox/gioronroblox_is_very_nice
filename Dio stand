function THEBEST()
  if selected == false or activu == true or appeared == true or POSU == true then
    return
  end
  local dipperhat = chr:FindFirstChild("DXD_DipperHat")
  local dipperrot
  if dipperhat then
    dipperrot = dipperhat.Handle.HatAttachment.Rotation
  end
  activu = true
