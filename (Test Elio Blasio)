fling = true
repeat
game.Players.LocalPlayer.Character["fff_fox"].Handle.Position = mouse.Hit.Position
task.wait(0.1)
game.Players.LocalPlayer.Character["fff_fox"].Handle.Position = game.Players.LocalPlayer.Character["Right Arm"].Position
task.wait(0.1)
until fling == false
end)

mouse.Button1Up:Connect(function()
    fling = false
                    game.Players.LocalPlayer.Character["fff_fox"].Handle.Position = game.Players.LocalPlayer.Character["Right Arm"].Position
end)

